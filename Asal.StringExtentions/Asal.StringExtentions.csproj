<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net6.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <PackageId>Asal.StringExtentions</PackageId>
    <Version>1.4.0</Version>
    <Title>Asal.StringExtentions</Title>
    <Authors>Hamza Asal</Authors>
    <Company>Hamza Asal</Company>
    <Product>Asal.StringExtentions</Product>
    <Description>A simple package that help you to call some string extentions to make some proccess on the string</Description>
    <PackageTags>String, extentions, Strings, Asal, email, string checker, string extensions, json extract</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/HamzaAl-Asal/Asal.StringExtentions</RepositoryUrl>
    <PackageReleaseNotes>Build new converter extentions:-
1- YamlToJson()
2- JsonToYaml()
You can read the ReadMe file for more information about these new extentions.</PackageReleaseNotes>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Desktop\ReadMe.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="YamlDotNet.NetCore" Version="1.0.0" />
  </ItemGroup>

</Project>
